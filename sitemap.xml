---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for page in site.pages %}
    {% unless page.visibility == "unlisted" or page.visibility == "discoverable" or page.visibility == "protected" %}
      <url>
        <loc>{{ site.url }}{{ page.url }}</loc>
        {% if page.date %}
          <lastmod>{{ page.date | date_to_xmlschema }}</lastmod>
        {% endif %}
      </url>
    {% endunless %}
  {% endfor %}

  {% for post in site.posts %}
    {% unless post.visibility == "unlisted" or post.visibility == "discoverable" or post.visibility == "protected" %}
      <url>
        <loc>{{ site.url }}{{ post.url }}</loc>
        <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
      </url>
    {% endunless %}
  {% endfor %}

  {% for item in site.content %}
    {% unless item.visibility == "unlisted" or item.visibility == "discoverable" or item.visibility == "protected" %}
      <url>
        <loc>{{ site.url }}{{ item.url }}</loc>
        {% if item.date %}
          <lastmod>{{ item.date | date_to_xmlschema }}</lastmod>
        {% endif %}
      </url>
    {% endunless %}
  {% endfor %}
</urlset>
